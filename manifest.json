{
  "manifest_version": 3,
  "name": "Google Dork Explorer",
  "version": "1.0",
  "description": "Unlock the power of Google Dorks with the Google Dork Explorer extension!",
  "author": "Suraj Sundar",
  "icons":{
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Google dork explorer",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popup.js"],
      "css": ["popup.css"]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
